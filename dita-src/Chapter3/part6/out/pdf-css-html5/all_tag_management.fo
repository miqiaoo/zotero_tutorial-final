                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">4.
                    </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">
                                                <fo:inline>为文献添加新标签</fo:inline>
                                                <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="40px" space-after="0em" space-before="0em" id="unique_3_Connect_42_substeps_ngk_vwd_ldc" chfox:nd-id="substeps_ngk_vwd_ldc">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">a.
                    </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                                                  <fo:inline>选中文献并打开标签面板</fo:inline>
                                                  </fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">b.
                    </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                                                  <fo:inline>点击"+" 图标，输入标签名称，按回车键完成。 <fo:inline chfox:inch="true" max-width="100%" width="300px" id="unique_3_Connect_42_image_lbd_1xd_ldc" chfox:nd-id="image_lbd_1xd_ldc"><fo:inline><fo:external-graphic src="file:/Users/mac/Desktop/2024%E5%85%A5%E5%AD%A6/%E8%AF%BE%E7%A8%8B/%E6%8A%80%E6%9C%AF%E5%86%99%E4%BD%9C/%E4%B8%AD%E6%9C%9F%E6%8A%A5%E5%91%8A/6.%20%E6%A0%87%E7%AD%BE%E7%AE%A1%E7%90%86/6-image/4%20%E7%BB%99%E4%B8%80%E7%AF%87%E6%96%87%E7%8C%AE%E6%B7%BB%E5%8A%A0%E6%A0%87%E7%AD%BE.png" max-width="100%" scaling="uniform" width="300px" content-width="scale-to-fit"/></fo:inline></fo:inline></fo:inline>
                                                  </fo:block></fo:list-item-body></fo:list-item>
