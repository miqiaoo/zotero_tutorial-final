<concept class="- topic/topic concept/concept " id="unique_7" topicrefclass="- map/topicref " nd:nd-id="concept_cnki_import" oid="concept_cnki_import" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">从中国知网导入文献条目</ph></title>
    <conbody class="- topic/body  concept/conbody ">
        <p class="- topic/p ">Zotero 提供插件支持直接从中国知网（CNKI）导入文献条目，便于高效获取学术资源。</p>
    </conbody>
</concept><task class="- topic/topic task/task " id="unique_8" topicrefclass="- map/topicref " nd:nd-id="task_import_cnki_entries" oid="task_import_cnki_entries" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">从知网导入文献条目</ph></title>
    <taskbody class="- topic/body task/taskbody ">
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">在中国知网找到所需文献。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.1a.png" id="unique_8_Connect_42_image_mql_g2m_ldc" nd:nd-id="image_mql_g2m_ldc" placement="inline" width="300"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">打开<uicontrol class="+ topic/ph ui-d/uicontrol ">“引用”</uicontrol>界面，右键点击并选择<uicontrol class="+ topic/ph ui-d/uicontrol ">“Save to Zotero (CNKI)”</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.1b.png" id="unique_8_Connect_42_image_vtr_h2m_ldc" nd:nd-id="image_vtr_h2m_ldc" placement="inline" width="300"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">选择目标文献库保存条目。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.1c.png" id="unique_8_Connect_42_image_g1k_j2m_ldc" nd:nd-id="image_g1k_j2m_ldc" placement="inline" width="150"/></info>
                <stepresult class="- topic/itemgroup task/stepresult "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.1d.png" id="unique_8_Connect_42_image_syw_l2m_ldc" nd:nd-id="image_syw_l2m_ldc" placement="inline" width="300"/></stepresult>
            </step>
        </steps>
    </taskbody>
</task></topic><topic class="- topic/topic " id="unique_9" topicrefclass="- map/topicref " nd:nd-id="import-with-identifiers" oid="import-with-identifiers" specializations="@props/deliveryTarget">
