<concept class="- topic/topic concept/concept " id="unique_14" topicrefclass="- map/topicref " nd:nd-id="concept_cnki_import" oid="concept_cnki_import" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">本地导入中文文献</ph></title>
    <conbody class="- topic/body  concept/conbody ">
        <p class="- topic/p ">本地文献导入可手动添加文件到 Zotero 文献库，适合管理非线上资源。</p>
    </conbody>
</concept><task class="- topic/topic task/task " id="unique_15" topicrefclass="- map/topicref " nd:nd-id="task_import_local_entries" oid="task_import_local_entries" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">本地导入中文文献</ph></title>
    <taskbody class="- topic/body task/taskbody ">
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击<uicontrol class="+ topic/ph ui-d/uicontrol ">“添加文件”</uicontrol>图标。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3a.png" id="unique_15_Connect_42_image_ypr_52m_ldc" nd:nd-id="image_ypr_52m_ldc" placement="inline" width="300"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">从本地选择所需文献。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3b.png" id="unique_15_Connect_42_image_hqv_v2m_ldc" nd:nd-id="image_hqv_v2m_ldc" placement="inline" width="300"/></info>
                <stepresult class="- topic/itemgroup task/stepresult ">文件已成功导入 Zotero。<image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3c.png" id="unique_15_Connect_42_image_ixc_x2m_ldc" nd:nd-id="image_ixc_x2m_ldc" placement="inline" width="300"/></stepresult>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">右键选中文献，依次点击<uicontrol class="+ topic/ph ui-d/uicontrol ">“知网助手”</uicontrol>，<uicontrol class="+ topic/ph ui-d/uicontrol ">“抓取文献元数据”</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3d.png" id="unique_15_Connect_42_image_wd4_y2m_ldc" nd:nd-id="image_wd4_y2m_ldc" placement="inline" width="300"/></info>
                <stepresult class="- topic/itemgroup task/stepresult ">成功抓取文献的元数据并添加至 Zotero。<image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3e.jpg" id="unique_15_Connect_42_image_grv_z2m_ldc" nd:nd-id="image_grv_z2m_ldc" placement="inline" width="300"/></stepresult>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">右键点击条目，选择<uicontrol class="+ topic/ph ui-d/uicontrol ">“添加到分类”</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3f.png" id="unique_15_Connect_42_image_m3g_bfm_ldc" nd:nd-id="image_m3g_bfm_ldc" placement="inline" width="300"/></info>
                <stepresult class="- topic/itemgroup task/stepresult ">文献已成功添加到目标分类。<image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3g.png" id="unique_15_Connect_42_image_vkv_cfm_ldc" nd:nd-id="image_vkv_cfm_ldc" placement="inline" width="250"/></stepresult>
            </step>
        </steps>
    </taskbody>
</task><troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_16" topicrefclass="- map/topicref " nd:nd-id="troubleshooting_duplicate_entries" oid="troubleshooting_duplicate_entries" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">文献库中出现重复条目</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">Zotero 文献库中出现重复条目。<image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3h.png" id="unique_16_Connect_42_image_ccq_2fm_ldc" nd:nd-id="image_ccq_2fm_ldc" placement="inline" width="300"/></p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">文献重复导入。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">用户</responsibleParty>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">打开<uicontrol class="+ topic/ph ui-d/uicontrol ">“重复条目”</uicontrol>文献库。 <image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3i.png" id="unique_16_Connect_42_image_hzt_ffm_ldc" nd:nd-id="image_hzt_ffm_ldc" placement="inline" width="300"/></cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">选中其中一条，重复文献将自动被选中。<image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3j.png" id="unique_16_Connect_42_image_avb_hfm_ldc" nd:nd-id="image_avb_hfm_ldc" placement="inline" width="300"/></cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">点击右上角的<uicontrol class="+ topic/ph ui-d/uicontrol ">“合并条目”</uicontrol>完成操作。<image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.3k.png" id="unique_16_Connect_42_image_mfs_3fm_ldc" nd:nd-id="image_mfs_3fm_ldc" placement="inline" width="300"/></cmd>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
