<concept class="- topic/topic concept/concept " id="unique_10" topicrefclass="- map/topicref " nd:nd-id="concept_identifiers_import" oid="concept_identifiers_import" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">通过文献标识符导入题录</ph></title>
    <conbody class="- topic/body  concept/conbody ">
        <p class="- topic/p ">文献标识符（如 DOI 或 ISBN）是快速导入条目的关键信息。</p>
    </conbody>
</concept><task class="- topic/topic task/task " id="unique_11" topicrefclass="- map/topicref " nd:nd-id="task_import_by_identifier" oid="task_import_by_identifier" specializations="@props/deliveryTarget">
        <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">使用文献标识符导入条目</ph></title>
        <taskbody class="- topic/body task/taskbody ">
            <steps class="- topic/ol task/steps ">
                <step class="- topic/li task/step ">
                    <cmd class="- topic/ph task/cmd ">点击 Zotero 工具栏的标识符导入图标。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.2a.png" id="unique_11_Connect_42_image_uq1_42m_ldc" nd:nd-id="image_uq1_42m_ldc" placement="inline" width="300"/></info>
                </step>
                <step class="- topic/li task/step ">
                    <cmd class="- topic/ph task/cmd ">输入标识符，例如：<uicontrol class="+ topic/ph ui-d/uicontrol ">doi: 10.1007/s43545-021-00273-9</uicontrol>，然后回车。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.2b.png" id="unique_11_Connect_42_image_l33_p2m_ldc" nd:nd-id="image_l33_p2m_ldc" placement="inline" width="250"/></info>
                    <stepresult class="- topic/itemgroup task/stepresult "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.2c.png" id="unique_11_Connect_42_image_c5y_q2m_ldc" nd:nd-id="image_c5y_q2m_ldc" placement="inline" width="300"/></stepresult>
                </step>
            </steps>
        </taskbody>
    </task><troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_12" topicrefclass="- map/topicref " nd:nd-id="troubleshooting_can_t_find_identifier" oid="troubleshooting_can_t_find_identifier" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">无法找到指定标识符</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">Zotero 提示找不到标识符。</p>
            <p class="- topic/p "><image class="- topic/image " href="file:/Users/freyaaf/Desktop/zotero-part4_part5/part4/4-image/4.2.2d.png" id="unique_12_Connect_42_image_bkc_t2m_ldc" nd:nd-id="image_bkc_t2m_ldc" placement="inline" width="300"/></p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">网络连接中断或 VPN 干扰。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">用户</responsibleParty>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">确保网络连接正常。</cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">关闭 VPN。</cmd>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
    </troublebody>
</troubleshooting></topic><topic class="- topic/topic " id="unique_13" topicrefclass="- map/topicref " nd:nd-id="local-import" oid="local-import" specializations="@props/deliveryTarget">
