    <topicref is-chapter="true" class="- map/topicref " href="#unique_1" type="topic"><topicmeta class="- map/topicmeta " data-topic-id="_2_2_install_plugin_0"><resourceid appid="_2_2_install_plugin_0" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_1" class="- topic/navtitle ">2.2 install plugin</navtitle><?ditaot gentext?></topicmeta><topicref class="- map/topicref " href="#unique_2" type="concept"><topicmeta class="- map/topicmeta " data-topic-id="concept_ek5_tcj_mdc"><resourceid appid="concept_ek5_tcj_mdc" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_2" class="- topic/navtitle ">2.2.1 Zotero在浏览器中的插件安装</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "/></topicmeta><topicref class="- map/topicref " href="#unique_3" type="task"><topicmeta class="- map/topicmeta " data-topic-id="task_rqn_2dj_mdc"><resourceid appid="task_rqn_2dj_mdc" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_3" class="- topic/navtitle ">a 在Chrome中安装Zotero的浏览器插件</navtitle><?ditaot gentext?></topicmeta></topicref><topicref class="- map/topicref " href="#unique_4" type="troubleshooting"><topicmeta class="- map/topicmeta " data-topic-id="trouble_kt4_5gj_mdc"><resourceid appid="trouble_kt4_5gj_mdc" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_4" class="- topic/navtitle ">b 无法在Chrome中安装Zotero的浏览器插件</navtitle><?ditaot gentext?></topicmeta></topicref></topicref><topicref class="- map/topicref " href="#unique_5" type="concept"><topicmeta class="- map/topicmeta " data-topic-id="chrome_plugin_operating"><resourceid appid="chrome_plugin_operating" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_5" class="- topic/navtitle ">2.2.2 Zotero的浏览器插件使用</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "/></topicmeta><topicref class="- map/topicref " href="#unique_6" type="task"><topicmeta class="- map/topicmeta " data-to